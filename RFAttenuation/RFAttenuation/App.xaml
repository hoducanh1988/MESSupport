<Application x:Class="RFAttenuation.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RFAttenuation"
             StartupUri="MainWindow.xaml">
    
    <Application.Resources>
        <ResourceDictionary>
            <SolidColorBrush x:Key="ColorMouseOver" Color="#fffc9e"/>
            <!--<SolidColorBrush x:Key="ColorBorder" Color="#20aefa"/>-->
            <SolidColorBrush x:Key="ColorBorder" Color="#777777"/>
            <SolidColorBrush x:Key="ColorTextWaiting" Color="Orange"/>
            <SolidColorBrush x:Key="ColorTextPass" Color="Lime"/>
            <SolidColorBrush x:Key="ColorTextFail" Color="Red"/>

        </ResourceDictionary>
    </Application.Resources>
</Application>
